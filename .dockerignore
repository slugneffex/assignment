# Ignore node_modules directory as it will be installed in the Docker container
node_modules

# Ignore log files
*.log

# Ignore Dockerfile and Docker Compose files themselves
Dockerfile
docker-compose.yml

# Ignore .env files containing environment variables
.env
.env.local
.env.*.local

# Ignore npm and yarn configuration files
.npmrc
.yarnrc

